aws_region  = "us-east-1"
environment = "dev"

# Set from terraform/02_queues outputs
notification_queue_url = "https://sqs.us-east-1.amazonaws.com/123456789012/audiotrans-dev-notification-queue"
notification_queue_arn = "arn:aws:sqs:us-east-1:123456789012:audiotrans-dev-notification-queue"

# Set from terraform/01_database outputs
users_table_name = "audiotrans-dev-users"
users_table_arn  = "arn:aws:dynamodb:us-east-1:123456789012:table/audiotrans-dev-users"
jobs_table_name  = "audiotrans-dev-jobs"
jobs_table_arn   = "arn:aws:dynamodb:us-east-1:123456789012:table/audiotrans-dev-jobs"

# Set from terraform/03_storage outputs
transcript_bucket_name = "audiotrans-dev-transcripts-123456789012"
transcript_bucket_arn  = "arn:aws:s3:::audiotrans-dev-transcripts-123456789012"

# Must be a verified SES identity (especially in sandbox)
sender_email = "noreply@example.com"
sendgrid_api_key = ""

lambda_zip_path = "../../backend/notify/notify_lambda.zip"
lambda_timeout_seconds = 60
lambda_memory_mb = 256
download_url_ttl_seconds = 86400
batch_size = 10
